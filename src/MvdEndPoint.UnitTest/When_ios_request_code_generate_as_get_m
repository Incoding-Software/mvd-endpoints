#import "GetCustomerRequest.h"

@implementation GetCustomerRequest
	@synthesize Message;    
	@synthesize Title;    
	@synthesize Ids;    

-(NSString *)asString
{
 return [NSString stringWithFormat:@"incType=%@&Message=%@&Title=%@&Ids=%@",
                         @"GetCustomerQuery" ,[self Message],[self Title],[self Ids]];  
}
@end

