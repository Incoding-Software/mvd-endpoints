import android.content.Context;
import org.apache.http.HttpResponse;
import org.apache.http.NameValuePair;
import org.apache.http.message.BasicNameValuePair;
import java.io.IOException;
import java.util.ArrayList;
import java.util.List;
import Incoding.IncodingHelper;

public class GetCustomerRequest {

    public TheSameString Message;
    public Number Title;
     

   public HttpResponse execute(Context context) throws IOException {       
	List<NameValuePair> parameters = new ArrayList<NameValuePair>();
	    parameters.add(new BasicNameValuePair(Message, String.valueOf(TheSameString)));
        parameters.add(new BasicNameValuePair(Title, String.valueOf(Number)));
       
	return IncodingHelper.Execute(context, false, "http://localhost/Dispatcher", parameters);
   } 
                                         
}