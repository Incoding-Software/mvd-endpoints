
using System;
using System.Text;

namespace MvdEndPoint.UnitTest
{
    public class FakeQuery:HttpMessageBase
    {
                 public String Login {get;set;}
                 public Int32 Count {get;set;}
                 public Double[] Values {get;set;}
            

		public class Response
        {		  
	    	       public class MyClass
	       {
	        			public String Login {get;set;}
						public Int32 Count {get;set;}
						public Double[] Values {get;set;}
				       }        
          
        				 
                public String Login {get;set;}
                 public Int32 Count {get;set;}
                 public Double[] Values {get;set;}
           

        }

        public async void Execute(Action<Response[]> onSuccess, Action onError = null, Action<ModelState[]> onValidation = null)
        {
            var postData = new Dictionary<string, object>();
            postData.Add("incTypes", "FakeQuery");
		    			    postData.Add("Login", this.Login);			             
            			    postData.Add("Count", this.Count);			             
            			    postData.Add("Values", this.Values);			             
              
            await PostAwait(false, onSuccess, onError, onValidation, postData);
        }
    }
}

