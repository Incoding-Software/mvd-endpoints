          <add key="WaiterOrderDetailsInterval" value="10000" />
          <add key="WaiterAlertWindowInterval" value="10000" />
          <add key="WaiterBillingInterval" value="10000" />
      <!-- End Waiter-->
  <!-- End Interval -->
</appSettings>