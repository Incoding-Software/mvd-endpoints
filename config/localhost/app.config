          <add key="KitchenBarActivityOrdersInterval" value="10000" />
          <add key="KitchenBarOrderDetailsInterval" value="10000" />
      <!-- End KitchenBar-->
      <!-- Begin Waiter -->
          <add key="WaiterDeviceInterval"  value="10000" />
