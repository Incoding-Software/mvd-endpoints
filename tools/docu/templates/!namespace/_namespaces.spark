<nav id="namespaces">
	<h2 class="fixed">Namespaces</h2>
	<div class="scroll">
		<ul>
	    <li each="var ns in Namespaces.Where(r=>r.Types.Any(s=>s.HasDocumentation))">	
        <if condition="ns == Namespace">
          ${Format(ns, class => "current")}
        <else />
          ${Format(ns)}
        </if>
      </li>
		</ul>
	</div>
</nav>